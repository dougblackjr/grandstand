<p id="notice"><%= notice %></p>

<h1>Listing Songs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Text</th>
      <th>Theme</th>
      <th>Tempo</th>
      <th>Key</th>
      <th>Cclisongno</th>
      <th>Scripture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.author %></td>
        <td><%= song.text %></td>
        <td><%= song.theme %></td>
        <td><%= song.tempo %></td>
        <td><%= song.key %></td>
        <td><%= song.CCLISongNo %></td>
        <td><%= song.scripture %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
